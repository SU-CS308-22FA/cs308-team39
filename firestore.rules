rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /merchandises {
      allow read: if true;
      allow write: if request.auth.uid != null;
    }
    match /users {
      allow read: if request.auth.uid != null;
      allow write: if true;
    }
  }
}
//firebase deploy --only firestore (to set these rules.)